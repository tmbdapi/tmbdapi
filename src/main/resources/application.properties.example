server.port=8081
# spring datasource url, username, pass
spring.datasource.driver-class-name=com.mysql.jdbc.Driver
spring.datasource.url=jdbc:mysql://localhost:3306/tutor_media?createDatabaseIfNotExist=true&useUnicode=yes&characterEncoding=UTF-8&useSSL=false
spring.datasource.username={username}
spring.datasource.password={password}

# spring hibernate dialect for Mysql
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL5Dialect

# spring jpa ddl-auto for validating db-schema
spring.jpa.hibernate.ddl-auto=validate

# ========================================
# = Flyway Configuration
# ========================================
flyway.locations = classpath:db/migration
flyway.url = jdbc:mysql://127.0.0.1:3306/
flyway.schemas= tutor_media
flyway.user = 
flyway.password = 